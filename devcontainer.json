{
  "name": "Intro FP",
  "image": "agerdes/intro-fp:latest",
  "customizations": {
    "vscode": {
      "extensions": [
        "justusadam.language-haskell"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "zsh",
        "terminal.integrated.profiles.linux": {
          "zsh": {
            "path": "/bin/zsh"
          }
        }
      }
    }
  }
}
